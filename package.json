{
  "name": "linkedin-job-analyzer",
  "version": "1.0.0",
  "description": "Chrome extension using AI to generate structured summaries of LinkedIn job postings",
  "main": "src/background.js",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "log-bridge": "node dev-tools/start-log-bridge.js",
    "inject-bridge": "./dev-tools/inject-bridge.sh inject",
    "remove-bridge": "./dev-tools/inject-bridge.sh remove",
    "bridge-status": "./dev-tools/inject-bridge.sh status",
    "dev": "npm run inject-bridge && npm run log-bridge",
    "dev:clean": "npm run remove-bridge"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.9",
    "vitest": "^2.1.9",
    "ws": "^8.18.3"
  },
  "keywords": [
    "chrome-extension",
    "linkedin",
    "ai",
    "job-analysis",
    "openai"
  ],
  "author": "Marco",
  "license": "MIT"
}
