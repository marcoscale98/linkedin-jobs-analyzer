{
  "name": "linkedin-job-analyzer",
  "version": "1.0.0",
  "description": "Chrome extension using AI to generate structured summaries of LinkedIn job postings",
  "main": "src/background.js",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "logs": "node dev-tools/simple-log-server.js",
    "add-bridge": "./dev-tools/add-console-bridge.sh add",
    "remove-bridge": "./dev-tools/add-console-bridge.sh remove"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.9",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "chrome-extension",
    "linkedin",
    "ai",
    "job-analysis",
    "openai"
  ],
  "author": "Marco",
  "license": "MIT"
}
